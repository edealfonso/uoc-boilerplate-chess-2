{
  "name": "chess-club-2",
  "version": "1.0.0",
  "private": true,
  "description": "Chess Club static page based on Boilerplate for Advanced HTML/CSS Tools UOC students, with Tailwind and PostCSS",
  "scripts": {
    "parcel:serve": "parcel src/index.html -p 8123 --target web --open",
    "parcel:build": "parcel build src/index.html --target web --no-source-maps --no-cache",
    "clean": "rimraf dist .cache .cache-loader .parcel-cache",
    "dev": "npm-run-all clean parcel:serve",
    "build": "npm-run-all clean parcel:build",
    "test": "echo 'Everything is working as expected âœ…\nStart working on your project by running \\033[1mnpm run dev\\033[0m'"
  },
  "web": "dist/index.html",
  "targets": {
    "web": {
      "context": "browser",
      "publicUrl": "./",
      "engines": {
        "browsers": "last 2 versions, > 0.5%, not dead"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/edealfonso/uoc-boilerplate-chess-2.git"
  },
  "author": {
    "name": "Elsa de Alfonso",
    "email": "elsa.de.alfonso@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/edealfonso/uoc-boilerplate-chess-2/issues"
  },
  "homepage": "https://github.com/edealfonso/uoc-boilerplate-chess-2#readme",
  "devDependencies": {
    "@parcel/transformer-sass": "^2.8.3",
    "autoprefixer": "^10.4.14",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.8.3",
    "postcss": "^8.4.23",
    "postcss-preset-env": "^7.8.3",
    "posthtml-include": "^1.7.4",
    "rimraf": "^3.0.2",
    "sharp": "0.31.1",
    "tailwindcss": "^3.3.2"
  }
}
